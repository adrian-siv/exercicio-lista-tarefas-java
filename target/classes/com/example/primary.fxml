<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" maxHeight="380.0" maxWidth="355.0" prefHeight="380.0" prefWidth="355.0" spacing="20.0" style="-fx-background-color: #EC8343;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.PrimaryController">
   <children>
      <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Lista de Compras">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Text>
      <VBox alignment="CENTER" prefHeight="84.0" prefWidth="249.0">
         <children>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="255.0" spacing="40.0">
               <children>
                  <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Produto">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Text>
                  <TextField fx:id="inputProduto" prefHeight="25.0" prefWidth="163.0" promptText="Ex.: suco" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="255.0" spacing="40.0">
               <children>
                  <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Setor">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Text>
                  <TextField fx:id="inputSetor" prefHeight="25.0" prefWidth="186.0" promptText="Ex.: suco" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Button mnemonicParsing="false" text="Adicionar Produto" />
      <ListView prefHeight="162.0" prefWidth="249.0" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
